# Do not remove this line or mod_rewrite rules and search engine friendly URLs will stop working
RewriteBase /
ErrorDocument 404 /404page.php
Options +FollowSymlinks

# Switching the rewrite engine on
RewriteEngine on

# Setting the lessons urls
RewriteRule ^lesson/(.+) /content.php?lesson=$1

# Individual Pages
RewriteRule ^c/lessons$ /content.php
RewriteRule ^c/about$ /about.php